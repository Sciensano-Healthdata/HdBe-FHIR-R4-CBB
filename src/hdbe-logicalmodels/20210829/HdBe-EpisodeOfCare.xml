                        <valueCode value="nl-NL"/>
                    </extension>
                    <extension url="content">
                        <valueMarkdown value="De datum die het begin van de zorgepisode markeert. In het algemeen is dit de datum van het eerste contact van de patiënt met de zorgverlener in het kader van het gezondheidsprobleem. "/>
                    </extension>
                </extension>
            </definition>
         <min value="0"/>
         <max value="1"/>
         <base>
                <path value="episode_of_care.start_date"/>
                <min value="0"/>
                <max value="1"/>
            </base>
         <type>
                <code value="dateTime"/>
