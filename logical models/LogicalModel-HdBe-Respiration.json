        "id": "Respiration.AdministeredOxygen.AdministrationDevice.MedicalDevice.Product.ProductId",
        "path": "Respiration.AdministeredOxygen.AdministrationDevice.MedicalDevice.Product.ProductId",
        "short": "ProductID",
        "_short": {
