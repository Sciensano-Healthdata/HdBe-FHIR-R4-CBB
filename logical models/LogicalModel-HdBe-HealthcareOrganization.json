      {
        "id": "HealthcareOrganization.HealthcareOrganizationIdentificationNumber",
        "path": "HealthcareOrganization.HealthcareOrganizationIdentificationNumber",
        "short": "HealthcareOrganizationIdentificationNumber",
        "_short": {
          "extension": [
            {
