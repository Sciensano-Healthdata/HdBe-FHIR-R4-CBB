    "extension": [
      {
        "url": "http://hl7.org/fhir/StructureDefinition/translation",
        "extension": [
          {
            "url": "lang",
            "valueCode": "nl-NL"
          },
          {
            "url": "content",
            "valueMarkdown": "Adresgegevens bevatten de gegevens waar een persoon (tijdelijk of vast) woont of waar een gebouw zich bevindt.\r\nDit is een subbouwsteen.\r\n#### Purpose\r\nAdres informatie wordt gebruik bij het bezoeken van personen of organisaties en ten behoeve van het adresseren van brieven."
          }
        ]
      }
    ]
