        "_short": {
          "extension": [
            {
              "extension": [
                {
                  "url": "lang",
                  "valueCode": "nl-NL"
                },
                {
                  "url": "content",
                  "valueMarkdown": "PackYears"
                }
              ],
              "url": "http://hl7.org/fhir/StructureDefinition/translation"
            },
            {
