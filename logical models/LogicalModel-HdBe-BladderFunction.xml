      <element id="BladderFunction.UrineCatheter.MedicalDevice.Product.ProductId">
         <path value="BladderFunction.UrineCatheter.MedicalDevice.Product.ProductId"/>
         <short value="ProductID">
            <extension url="http://hl7.org/fhir/StructureDefinition/translation">
               <extension url="lang">
                  <valueCode value="nl-NL"/>
