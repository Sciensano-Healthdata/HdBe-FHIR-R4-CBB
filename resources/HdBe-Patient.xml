      <type>
        <code value="integer" />
      </type>
      <mapping>
        <identity value="HdBe-Patient" />
        <map value="patient.multiple_birth_order" />
      </mapping>
      <mapping>
        <identity value="HdBe-Patient" />
        <map value="patient.multiple_birth_indicator" />
      </mapping>
